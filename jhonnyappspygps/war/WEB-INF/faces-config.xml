<?xml version="1.0"?>
<faces-config version="2.0" 
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

 	<managed-bean>
	  <managed-bean-name>principalBean</managed-bean-name>
	  <managed-bean-class>es.jhonny.spygps.backingbean.PrincipalBean</managed-bean-class>
	  <managed-bean-scope>request</managed-bean-scope>
	 </managed-bean>
	 <managed-bean>
	  <managed-bean-name>loginBean</managed-bean-name>
	  <managed-bean-class>es.jhonny.spygps.backingbean.LoginBean</managed-bean-class>
	  <managed-bean-scope>request</managed-bean-scope>
	 </managed-bean>
	 <managed-bean>
	  <managed-bean-name>mapaBean</managed-bean-name>
	  <managed-bean-class>es.jhonny.spygps.backingbean.MapaBean</managed-bean-class>
	  <managed-bean-scope>request</managed-bean-scope>
	 </managed-bean>
	 <managed-bean>
	  <managed-bean-name>contactoBean</managed-bean-name>
	  <managed-bean-class>es.jhonny.spygps.backingbean.ContactoBean</managed-bean-class>
	  <managed-bean-scope>request</managed-bean-scope>
	 </managed-bean>
	 <managed-bean>
	  <managed-bean-name>cuentaBean</managed-bean-name>
	  <managed-bean-class>es.jhonny.spygps.backingbean.CuentaBean</managed-bean-class>
	  <managed-bean-scope>request</managed-bean-scope>
	 </managed-bean>
	 <managed-bean>
	  <managed-bean-name>enlacesBean</managed-bean-name>
	  <managed-bean-class>es.jhonny.spygps.backingbean.EnlacesBean</managed-bean-class>
	  <managed-bean-scope>request</managed-bean-scope>
	 </managed-bean>
	 <managed-bean>
	  <managed-bean-name>productosBean</managed-bean-name>
	  <managed-bean-class>es.jhonny.spygps.backingbean.ProductosBean</managed-bean-class>
	  <managed-bean-scope>request</managed-bean-scope>
	 </managed-bean>
	 <managed-bean>
	  <managed-bean-name>registroBean</managed-bean-name>
	  <managed-bean-class>es.jhonny.spygps.backingbean.RegistroBean</managed-bean-class>
	  <managed-bean-scope>request</managed-bean-scope>
	 </managed-bean>
	 <managed-bean>
	  <managed-bean-name>politicasBean</managed-bean-name>
	  <managed-bean-class>es.jhonny.spygps.backingbean.PoliticasBean</managed-bean-class>
	  <managed-bean-scope>request</managed-bean-scope>
	 </managed-bean>
	 <managed-bean>
	  <managed-bean-name>terminosBean</managed-bean-name>
	  <managed-bean-class>es.jhonny.spygps.backingbean.TerminosBean</managed-bean-class>
	  <managed-bean-scope>request</managed-bean-scope>
	 </managed-bean>
	 <managed-bean>
	  <managed-bean-name>usuarioBean</managed-bean-name>
	  <managed-bean-class>es.jhonny.spygps.backingbean.UsuarioBean</managed-bean-class>
	  <managed-bean-scope>session</managed-bean-scope>
	 </managed-bean>
	 <navigation-rule>
	  <navigation-case>
	   <display-name>principal.jsf</display-name>
	   <from-outcome>pagina_inicio</from-outcome>
	   <to-view-id>/pages/principal.xhtml</to-view-id>
	   <redirect/>
	  </navigation-case>
	 </navigation-rule>
	 <navigation-rule>
	  <navigation-case>
	   <display-name>mapa.jsf</display-name>
	   <from-outcome>pagina_mapa</from-outcome>
	   <to-view-id>/pages/secure/mapa.xhtml</to-view-id>
	   <redirect/>
	  </navigation-case>
	 </navigation-rule>
	 <navigation-rule>
	  <navigation-case>
	   <display-name>cuenta.jsf</display-name>
	   <from-outcome>pagina_cuenta</from-outcome>
	   <to-view-id>/pages/secure/cuenta.xhtml</to-view-id>
	   <redirect/>
	  </navigation-case>
	 </navigation-rule>
	 <navigation-rule>
	  <navigation-case>
	   <display-name>contacto.jsf</display-name>
	   <from-outcome>pagina_contacto</from-outcome>
	   <to-view-id>/pages/contacto.xhtml</to-view-id>
	   <redirect/>
	  </navigation-case>
	 </navigation-rule>
	 <navigation-rule>
	  <navigation-case>
	   <display-name>registro.jsf</display-name>
	   <from-outcome>pagina_registro</from-outcome>
	   <to-view-id>/pages/registro.xhtml</to-view-id>
	   <redirect/>
	  </navigation-case>
	 </navigation-rule>
	 <navigation-rule>
	  <navigation-case>
	   <display-name>login.jsf</display-name>
	   <from-outcome>pagina_login</from-outcome>
	   <to-view-id>/pages/login.xhtml</to-view-id>
	   <redirect/>
	  </navigation-case>
	 </navigation-rule>
	 <navigation-rule>
	  <navigation-case>
	   <display-name>productos.jsf</display-name>
	   <from-outcome>pagina_productos</from-outcome>
	   <to-view-id>/pages/productos.xhtml</to-view-id>
	   <redirect/>
	  </navigation-case>
	 </navigation-rule>
	 <navigation-rule>
	  <navigation-case>
	   <display-name>enlaces.jsf</display-name>
	   <from-outcome>pagina_enlaces</from-outcome>
	   <to-view-id>/pages/enlaces.xhtml</to-view-id>
	   <redirect/>
	  </navigation-case>
	 </navigation-rule>
	 <navigation-rule>
	  <navigation-case>
	   <display-name>politicas.jsf</display-name>
	   <from-outcome>pagina_politicas</from-outcome>
	   <to-view-id>/pages/politicas.xhtml</to-view-id>
	   <redirect/>
	  </navigation-case>
	 </navigation-rule>
	 <navigation-rule>
	  <navigation-case>
	   <display-name>terminos.jsf</display-name>
	   <from-outcome>pagina_terminos</from-outcome>
	   <to-view-id>/pages/terminos.xhtml</to-view-id>
	   <redirect/>
	  </navigation-case>
	 </navigation-rule>
	 
	 <application>
	  <locale-config>
	   <default-locale>es</default-locale>
	   <supported-locale>en</supported-locale>
	  </locale-config>
	  <message-bundle>textos</message-bundle>
	 </application>
</faces-config>
